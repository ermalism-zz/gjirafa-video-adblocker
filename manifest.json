{
  "name": "Gjirafa Video AdBlocker",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "AdBlocker for Gjirafa Video",
  "homepage_url": "https://github.com/ermalism",
  "browser_action": {
    "default_title": "Gjirafa Video AdBlocker"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.gjirafa.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "*://*.gjirafa.com/*"
  ]
}
